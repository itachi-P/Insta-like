{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "essential": true,
      "image": "568975345534.dkr.ecr.ap-northeast-1.amazonaws.com/php-fpm:latest",
      "memory": 128,
      "mountPoints": [
        {
          "containerPath": "/var/www",
          "sourceVolume": "src"
        },
        {
          "containerPath": "/usr/local/var/log",
          "sourceVolume": "awseb-logs-php-fpm"
        }
      ],
      "name": "php-fpm"
    },
    {
      "essential": true,
      "image": "568975345534.dkr.ecr.ap-northeast-1.amazonaws.com/nginx:latest",
      "links": [
        "php-fpm"
      ],
      "memory": 128,
      "mountPoints": [
        {
          "containerPath": "/var/www",
          "sourceVolume": "src"
        },
        {
          "containerPath": "/var/log/nginx",
          "sourceVolume": "awseb-logs-nginx"
        }
      ],
      "name": "nginx",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "volumes": [
    {
      "host": {
        "sourcePath": "/var/app/current/src"
      },
      "name": "src"
    }
  ]
}
